{
  "name": "Save to WebSite",
  "version": "0.3.7",
  "manifest_version": 2,
  "description": "Save to WebSite",
  "options_page": "options.html",
  "background": { 
    "page": "background.html" 
  },
  "permissions": [
    "*://*/",
    "tabs",
    "contextMenus",
    "unlimitedStorage",
    "notifications"
  ],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "64": "icon/64.png",
    "64": "icon/64sad.png",
    "128": "icon/128.png",
    "512": "icon/512.png"
  },
  "web_accessible_resources": [
    "icon/16.png",
	"icon/32.png",
	"icon/48.png",
	"icon/64.png",
	"icon/64sad.png",
	"icon/128.png",
	"icon/512.png"
  ]
}
